<?php

/**
 * Implements hook_preprocess_menu().
 */
function copyrights_block_preprocess_menu(&$variables) {
  if ($variables["menu_name"] == 'social-network') {
    $config = \Drupal::entityTypeManager()
      ->getStorage('config_pages')
      ->load('global_configurations');
    $token_text = $config->get('field_social_label')->value;
    $variables['social_label'] = $token_text;
  }
}
